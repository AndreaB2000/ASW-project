{
  "entryPoints": ["src/server", "src/client"],
  "entryPointStrategy": "expand",
  "out": "dist/docs/typedoc",
  "exclude": [
    "**/*.+(config|spec|e2e).ts",
    "**/node_modules/**",
    "**/*.vue",
    "./src/client/cypress/**",
    "./src/client/env.d.ts"
  ],
  "includeVersion": true,
  "tsconfig": "tsconfig.json",
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "hideGenerator": true
}
